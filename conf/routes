# Home page
GET     /                         Application.index

# LOGIN
GET     /login                    Application.login

# Ignore favicon requests
GET     /favicon.ico              404

# Static files
GET     /public/                  staticDir:public

# Atendentes
GET     /Atendentes/detalhar/{id} Atendentes.detalhar

# Atendentes
GET     /Pacientes/detalhar/{id} Pacientes.detalhar

# Catch all 
*       /{controller}/{action}    {controller}.{action}
